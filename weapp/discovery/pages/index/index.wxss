page {
position: relative;
width: 100vw; /*相对单位 viewport width */
height: 100vh; /*相对单位 viewport heigth */
}

.container {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 80%;
  height: 80%;
  border: 3px solid #EBBCBC;
  margin: 0px auto;
  overflow: hidden;
  border-radius: 30px;
  text-align: center;
}
.title {
  color: red;
  font-size: 20px;
}